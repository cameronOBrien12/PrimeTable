#include <stdio.h>

//MULTIPLICATION TABLE IS MODULUS
int main( int argc, char *argv[] )  {

	//holds our number we give in command line
	int n ;
   
	//if there is valid argument
	if( argc == 2 ) 
	{	
	//CHANGE to int
	n = atoi (argv[1]);
	printf ("The number you have chosen is %d. \n *  | ",n);   
   	}
	//if more than one argument
  	else if( argc > 2 ) 
	{
        printf("Too many arguments supplied.\n");
  	}
   	else 
	{
	//if no argument
        printf("One argument expected.\n ");
   	}
